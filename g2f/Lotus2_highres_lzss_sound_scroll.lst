     2 				; LZSS Compressed SAP player for 16 match bits
     3 				; --------------------------------------------
     4 				;
     5 				; (c) 2020 DMSC -> adapted for pic_sound_scroll_tut by PPs
     6 				; Code under MIT license, see LICENSE file.
     7 				;
     8 				; This player uses:
     9 				;  Match length: 8 bits  (1 to 256)
    10 				;  Match offset: 8 bits  (1 to 256)
    11 				;  Min length: 2
    12 				;  Total match bits: 16 bits
    13 				;
    14 				; Compress using:
    15 				;  lzss -b 16 -o 8 -m 1 input.rsap test.lz12
    16 				;
    17 				; Assemble this file with MADS assembler, the compressed song is expected in
    18 				; the `test.lz16` file at assembly time.
    19 				;
    20 				; The player needs 256 bytes of buffer for each pokey register stored, for a
    21 				; full SAP file this is 2304 bytes.
    22 				;
    23
    24 				/*
    25 				Don't compile this alone, just use g2f or rasterconverter files, that added the correct lines!
    26 				*/
    26
    27
    28 				    org $c0
    29
    30 = 00C0			chn_copy    .ds     9
    31 = 00C9			chn_pos     .ds     9
    32 = 00D2			bptr        .ds     2
    33 = 00D4			cur_pos     .ds     1
    34 = 00D5			chn_bits    .ds     1
    35
    36 00D6-00E0> 01		bit_data    .byte   1
    37
    38 00D7			.proc get_byte
    39 00D7 AD 01 99		    lda song_data+1
    40 00DA E6 D8		    inc song_ptr
    41 00DC D0 02		    bne skip
    42 00DE E6 D9		    inc song_ptr+1
    43 00E0			skip
    44 00E0 60			    rts
    45 				.endp
    46 = 00D8			song_ptr = get_byte + 1
    47
    48
    49 = D200			POKEY = $D200
    50
    51 				    org PLAYER
    52 9000			buffers
    53 = 9000			    .ds 256 * 9
    54
    55 9900			song_data		;change filename to your musik file
    56 9900-BE8C> 01 00 00 00 +         ins     'PPs_bootup.lz16'
    57 A5D9			song_end
