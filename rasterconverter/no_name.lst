    90 A5F8			.proc play_frame
    91 A5F8 A9 90		    lda #>buffers
    92 A5FA 85 D3		    sta bptr+1
    93
    94 A5FC AD 00 99		    lda song_data
    95 A5FF 85 D5		    sta chn_bits
    96 A601 A2 08		    ldx #8
    97
    98 				    ; Loop through all "channels", one for each POKEY register
    99 A603			chn_loop:
   100 A603 46 D5		    lsr chn_bits
   101 A605 B0 29		    bcs skip_chn       ; C=1 : skip this channel
   102
   103 A607 B5 C0		    lda chn_copy, x    ; Get status of this stream
   104 A609 D0 16		    bne do_copy_byte   ; If > 0 we are copying bytes
   105
   106 				    ; We are decoding a new match/literal
   107 A60B 46 D6		    lsr bit_data       ; Get next bit
   108 A60D D0 06		    bne got_bit
   109 A60F 20 D7 00		    jsr get_byte       ; Not enough bits, refill!
   110 A612 6A			    ror                ; Extract a new bit and add a 1 at the high bit (from C set above)
   111 A613 85 D6		    sta bit_data       ;
   112 A615			got_bit:
   113 A615 20 D7 00		    jsr get_byte       ; Always read a byte, it could mean "match size/offset" or "literal byte"
   114 A618 B0 0F		    bcs store          ; Bit = 1 is "literal", bit = 0 is "match"
   115
   116 A61A 95 C9		    sta chn_pos, x     ; Store in "copy pos"
   117
   118 A61C 20 D7 00		    jsr get_byte
   119 A61F 95 C0		    sta chn_copy, x    ; Store in "copy length"
   120
   121 				                        ; And start copying first byte
   122 A621			do_copy_byte:
   123 A621 D6 C0		    dec chn_copy, x     ; Decrease match length, increase match position
   124 A623 F6 C9		    inc chn_pos, x
   125 A625 B4 C9		    ldy chn_pos, x
   126
   127 				    ; Now, read old data, jump to data store
   128 A627 B1 D2		    lda (bptr), y
   129
   130 A629			store:
   131 A629 A4 D4		    ldy cur_pos
   132 A62B 9D 00 D2		    sta POKEY, x        ; Store to output and buffer
   133 A62E 91 D2		    sta (bptr), y
   134
   135 A630			skip_chn:
   136 				    ; Increment channel buffer pointer
   137 A630 E6 D3		    inc bptr+1
   138
   139 A632 CA			    dex
   140 A633 10 CE		    bpl chn_loop        ; Next channel
   141
   142 A635 E6 D4		    inc cur_pos
   143 				.endp
